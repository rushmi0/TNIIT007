<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EFFAD7"
    tools:context=".Week5">

    <EditText
        android:id="@+id/nameInput2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="105dp"
        android:layout_y="126dp"
        android:ems="10"
        android:hint="name..."
        android:inputType="text" />

    <EditText
        android:id="@+id/scoreInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="105dp"
        android:layout_y="197dp"
        android:ems="10"
        android:hint="score..."
        android:inputType="text" />

    <Button
        android:id="@+id/sendbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="157dp"
        android:layout_y="284dp"
        android:text="SEND"
        app:cornerRadius="0dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="4dp"
        android:layout_y="5dp"
        android:background="#00FFFFFF"
        android:text="Page 5A"
        android:textColor="#7A7878"
        android:textSize="45dp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/C"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="148dp"
        android:layout_y="361dp"
        android:text="Button" />
</AbsoluteLayout>